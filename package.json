{
  "name": "obsidian-codex-plugin",
  "version": "1.0.0",
  "description": "Plugin para Obsidian que gera notas automaticamente a partir de PDFs usando Gemini AI",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["obsidian", "plugin", "ai", "gemini", "pdf", "notes"],
  "author": "Manus AI",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.25.0",
    "obsidian": "^1.2.5",
    "tslib": "2.4.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "pdfjs-dist": "^4.0.379"
  }
}

